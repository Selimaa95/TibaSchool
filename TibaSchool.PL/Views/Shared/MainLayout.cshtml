<!DOCTYPE html>

<html lang="en">

    <head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no">
    <!--===============================================================================================-->
    <title>@ViewData["Title"]</title>
    <!--===============================================================================================-->
    <link href="https://fonts.googleapis.com/css?family=Montserrat:400,700" rel="stylesheet">
    <!--===============================================================================================-->
    <link rel="icon" type="image/png" href="~/images/icons/logo2.png" />
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/vendor/bootstrap/css/bootstrap.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/fonts/font-awesome-4.7.0/css/font-awesome.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/fonts/themify/themify-icons.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/vendor/animate/animate.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/vendor/css-hamburgers/hamburgers.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/vendor/animsition/css/animsition.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/vendor/select2/select2.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/vendor/daterangepicker/daterangepicker.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/vendor/slick/slick.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/vendor/lightbox2/css/lightbox.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/css/util.css">
    <link rel="stylesheet" type="text/css" href="~/css/main.css">
    <!--===============================================================================================-->
</head>

    <body class="animsition">

       <!-- ************************** Header *****************************-->
        <!-- #region Header -->
        <!-- Header -->
        <header>
            <!-- Header desktop -->
            <div class="wrap-menu-header gradient1 trans-0-4">
                <div class="container h-full">
                    <div class="wrap_header trans-0-3">
                        <!-- Logo -->
                        <div class="logo">
                            <a asp-controller="Home" asp-action="Index">
                                <img src="/images/icons/logo.png" alt="IMG-LOGO" data-logofixed="/images/icons/logo2.png">
                            </a>
                        </div>

                        <!-- Menu -->
                        <div class="wrap_menu p-l-45 p-l-0-xl">
                            <nav class="menu">
                                <ul class="main_menu">
                                    <li>
                                        <a asp-controller="Home" asp-action="Index">Home</a>
                                    </li>

                                    <li>
                                        <a asp-controller="Home" asp-action="About">About</a>
                                    </li>

                                    <li>
                                        <a asp-controller="Home" asp-action="OurSchool">Our School</a>
                                    </li>
                                    <li>
                                        <a asp-controller="Home" asp-action="Subjects">Subjects</a>
                                    </li>

                                    <li>
                                        <a asp-controller="Home" asp-action="Gallery">Gallery</a>
                                    </li>

                                    <li>
                                        <a asp-controller="Home" asp-action="News">News</a>
                                    </li>

                                    <li>
                                        <a asp-controller="Home" asp-action="Appeals">Appeals</a>
                                    </li>

                                    <li>
                                        <a asp-controller="Home" asp-action="Admission">Admission</a>
                                    </li>

                                    <li>
                                        <a asp-controller="Contact" asp-action="Contact">Contact</a>
                                    </li>

                                    <li>
                                        <a href="http://schooleverywhere-tibaschool.com" target="_blank">School Login</a>
                                    </li>
                                </ul>
                            </nav>
                        </div>

                        <!-- Social -->
                        <div class="social flex-w flex-l-m p-r-20">

                            <a href="https://www.facebook.com/tibainternationalschool/"><i class="fa fa-facebook m-l-21" aria-hidden="true"></i></a>
                            <a href="#"><i class="fa fa-twitter m-l-21" aria-hidden="true"></i></a>

                            <button class="btn-show-sidebar m-l-33 trans-0-4"></button>
                        </div>
                    </div>
                </div>
            </div>
        </header>
        <!-- #endregion -->

        <!-- ******************************* Sidebar **********************-->
        <!-- #region Sidebar -->
        <!-- Sidebar -->
        <aside class="sidebar trans-0-4">
            <!-- Button Hide sidebar -->
            <button class="btn-hide-sidebar ti-close color0-hov trans-0-4"></button>
            <!-- - -->
            <ul class="menu-sidebar p-t-95 p-b-70">

               @* <li class="t-center m-b-13">
                    <a asp-controller="AdminPanel" asp-action="Index" class="txt19">Admin Panel</a>
                </li>*@
                <li class="t-center m-b-13">
                    <a asp-controller="Home" asp-action="Index" class="txt19">Home</a>
                </li>

                <li class="t-center m-b-13">
                    <a asp-controller="Home" asp-action="About" class="txt19">About</a>
                </li>

                <li class="t-center m-b-13">
                    <a asp-controller="Home" asp-action="OurSchool" class="txt19">Our School</a>
                </li>

                <li class="t-center m-b-13">
                    <a asp-controller="Home" asp-action="Subjects" class="txt19">Subjects</a>
                </li>

                <li class="t-center m-b-13">
                    <a asp-controller="Home" asp-action="Gallery" class="txt19">Gallery</a>
                </li>

                <li class="t-center m-b-13">
                    <a asp-controller="Home" asp-action="News" class="txt19">News</a>
                </li>

                <li class="t-center m-b-33">
                    <a asp-controller="Home" asp-action="Appeals" class="txt19">Appeals</a>
                </li>

                <li class="t-center m-b-33">
                    <a asp-controller="Home" asp-action="Admission" class="txt19">Admission</a>
                </li>

                <li class="t-center m-b-13">
                    <a asp-controller="Contact" asp-action="Contact" class="txt19">Contact</a>
                </li>



                <li class="t-center">
                    <!-- Button3 -->
                    <a href="http://schooleverywhere-tibaschool.com" target="_blank" class="btn3 flex-c-m size13 txt11 trans-0-4 m-l-r-auto">
                        School Login
                    </a>
                </li>
            </ul>

            <!-- - -->
            <div class="gallery-sidebar t-center p-l-60 p-r-60 p-b-40">
                <!-- - -->
                <h4 class="txt20 m-b-33">
                    SchoolEveryWhere
                </h4>

                <!-- Gallery -->
                <div class="wrap-gallery-sidebar flex-w ">
                    <a class="item-gallery-sidebar wrap-pic-w" href="~/images/photo-gallery-01.jpg" data-lightbox="gallery-footer">
                        <img src="~/images/photo-gallery-thumb-01.jpg" alt="GALLERY">
                    </a>




                </div>
            </div>
        </aside>
        <!-- #endregion -->

        <main role="main" class="pb-3">
            @RenderBody()
        </main>

        <!--***********************************Footer*******************-->
        <!-- #region Footer -->
        <footer class="bg1">
            <div class="container p-t-40 p-b-70">
                <div class="row">
                    <div class="col-sm-6 col-md-4 p-t-50">
                        <!-- - -->
                        <h4 class="txt13 m-b-33">
                            Contact Us
                        </h4>

                        <ul class="m-b-70">
                            <li class="txt14 m-b-14">
                                <i class="fa fa-map-marker fs-16 dis-inline-block size19" aria-hidden="true"></i>
                                instruction between 45 st and Mostafa Kamel..Miamy.Alex

                            </li>

                            <li class="txt14 m-b-14">
                                <i class="fa fa-phone fs-16 dis-inline-block size19" aria-hidden="true"></i>
                                (+2) 03 5366641
                            </li>

                            <li class="txt14 m-b-14">
                                <i class="fa fa-envelope fs-13 dis-inline-block size19" aria-hidden="true"></i>
                                info@tiba.school
                            </li>
                        </ul>

                        <!-- - -->

                    </div>

                    <div class="col-sm-6 col-md-4 p-t-50">
                        <!-- - -->
                        <h4 class="txt13 m-b-32">
                            Opening Times
                        </h4>

                        <ul>
                            <li class="txt14">
                                07:00 AM – 4:00 PM
                            </li>

                            <li class="txt14">
                                Every Day
                            </li>
                        </ul>
                    </div>

                    <div class="col-sm-6 col-md-4 p-t-50">
                        <!-- - -->
                        <h4 class="txt13 m-b-38">
                            SchoolEveryWhere
                        </h4>

                        <!-- Gallery footer -->
                        <div class="wrap-gallery-footer flex-w">
                            <a class="item-gallery-footer wrap-pic-w" href="~/images/photo-gallery-01.jpg" data-lightbox="gallery-footer">
                                <img src="~/images/photo-gallery-thumb-01.jpg" alt="GALLERY">
                            </a>

                        </div>

                    </div>
                </div>
            </div>

            <div class="end-footer bg2">
                <div class="container">
                    <div class="flex-sb-m flex-w p-t-22 p-b-22">
                        <div class="p-t-5 p-b-5">

                            <a href="https://www.facebook.com/tibainternationalschool/" class="fs-15 c-white"><i class="fa fa-facebook m-l-18" aria-hidden="true"></i></a>
                            <a href="#" class="fs-15 c-white"><i class="fa fa-twitter m-l-18" aria-hidden="true"></i></a>
                        </div>

                        <div class="txt17 p-r-20 p-t-5 p-b-5">
                            Copyright &copy; 2019 All rights reserved  |  This template Powered by <a href="http://www.wide-horizons.net/" target="_blank">Wide Horizons</a>
                        </div>
                    </div>
                </div>
            </div>
        </footer>


        <!-- Back to top -->
        <div class="btn-back-to-top bg0-hov" id="myBtn">
            <span class="symbol-btn-back-to-top">
                <i class="fa fa-angle-double-up" aria-hidden="true"></i>
            </span>
        </div>

        <!-- Container Selection1 -->
        <div id="dropDownSelect1"></div>

        <!-- Modal Video 01-->
        <div class="modal fade" id="modal-video-01" tabindex="-1" role="dialog" aria-hidden="true">

            <div class="modal-dialog" role="document" data-dismiss="modal">
                <div class="close-mo-video-01 trans-0-4" data-dismiss="modal" aria-label="Close">&times;</div>

                <div class="wrap-video-mo-01">
                    <div class="w-full wrap-pic-w op-0-0"><img src="~/images/icons/video-16-9.jpg" alt="IMG"></div>
                    <div class="video-mo-01">
                        <iframe src="https://www.youtube.com/embed/5k1hSu2gdKE?rel=0&amp;showinfo=0" allowfullscreen></iframe>
                    </div>
                </div>
            </div>
        </div>

        <!-- Footer -->
        <!-- #endregion -->

        <!-- ********************* Js Libraries *************************-->
        <!-- #region Js Libraries -->
        <!--===============================================================================================-->
        <script type="text/javascript" src="~/vendor/jquery/jquery-3.2.1.min.js"></script>
        @RenderSection("JaqueryValidation", required: false)
        <!--===============================================================================================-->
        <script type="text/javascript" src="~/vendor/animsition/js/animsition.min.js"></script>
        <!--===============================================================================================-->
        <script type="text/javascript" src="~/vendor/bootstrap/js/popper.js"></script>
        <script type="text/javascript" src="~/vendor/bootstrap/js/bootstrap.min.js"></script>
        <!--===============================================================================================-->
        <script type="text/javascript" src="~/vendor/select2/select2.min.js"></script>
        <!--===============================================================================================-->
        <script type="text/javascript" src="~/vendor/daterangepicker/moment.min.js"></script>
        <script type="text/javascript" src="~/vendor/daterangepicker/daterangepicker.js"></script>
        <!--===============================================================================================-->
        <script type="text/javascript" src="~/vendor/slick/slick.min.js"></script>
        <script type="text/javascript" src="~/js/slick-custom.js"></script>
        <!--===============================================================================================-->
        <script type="text/javascript" src="~/vendor/parallax100/parallax100.js"></script>
        <script type="text/javascript">
            $('.parallax100').parallax100();
        </script>
        <!--===============================================================================================-->
        <script type="text/javascript" src="~/vendor/countdowntime/countdowntime.js"></script>
        <!--===============================================================================================-->
        <script type="text/javascript" src="~/vendor/lightbox2/js/lightbox.min.js"></script>
        <!--===============================================================================================-->
        @RenderSection("JSContact", required: false)
        <!--===============================================================================================-->
        @RenderSection("JSLibrary", required: false)
        <!--===============================================================================================-->
        <script src="~/js/main.js"></script>
        <!-- #endregion -->

    </body>

</html>

